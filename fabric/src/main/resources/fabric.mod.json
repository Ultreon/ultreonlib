{
  "schemaVersion": 1,
  "id": "ultreonlib",
  "version": "${version}",
  "name": "Ultreon Modding Library",
  "description": "${description}",
  "authors": [
    "Qboi123"
  ],
  "contributors": [

  ],
  "contact": {
    "website": "https://ultreon.github.io/web/",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/ultreon-mod-lib",
    "repo": "https://gitlab.com/ultreon/ultreon-mod-lib",
    "issues": "https://gitlab.com/ultreon/ultreon-mod-lib/-/issues"
  },
  "license": "Ultreon API License v1.1",
  "icon": "ultreonlib.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ultreon.mods.lib.fabric.UltreonLibFabric"
    ],
    "client": [
      "com.ultreon.mods.lib.fabric.UltreonLibFabricClient"
    ],
    "modmenu": [
      "com.ultreon.mods.lib.fabric.UltreonLibModMenuIntegration"
    ]
  },
  "mixins": [
    "ultreonlib.mixins.json",
    "ultreonlib-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "minecraft": ">=${minecraftVersion} <1.20"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/ultreon-mod-lib",
        "modmenu.modrinth": "https://modrinth.com/mod/ultreonlib"
      },
      "badges": [ "library" ]
    }
  }
}
